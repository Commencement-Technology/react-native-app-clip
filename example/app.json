{
  "expo": {
    "name": "Clipper",
    "slug": "clipper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "example.clipper"
    },
    "plugins": [
      [
        "../app.plugin.js",
        {
          "name": "Clipper Clip",
          "bundleIdSuffix": "AppClip",
          "targetSuffix": "Clip",
          "groupIdentifier": "example.clipper",
          "deploymentTarget": "14.2",
          "requestEphemeralUserNotification": true,
          "requestLocationConfirmation": true,
          "appleSignin": true,
          "applePayMerchantIds": ["example.clipper"],
          "excludedPackages": ["@shopify/react-native-skia"]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          }
        }
      ]
    ]
  }
}
